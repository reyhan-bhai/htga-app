    "@mui/icons-material": "^6.4.4",
    "@mui/material": "^6.4.4",
    "@nextui-org/react": "^2.6.11",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/nodemailer": "^7.0.4",
    "firebase": "^10.13.2",
    "firebase-admin": "^13.1.0",
    "framer-motion": "^12.23.25",
    "geist": "^1.5.1",
    "jsonwebtoken": "^9.0.3",
    "next": "15.1.7",
    "nodemailer": "^7.0.11",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
