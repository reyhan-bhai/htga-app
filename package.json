    "@types/react-dom": "^19",
    "dotenv": "^16.4.7",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "postcss": "^8",
    "prettier": "^3.5.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
